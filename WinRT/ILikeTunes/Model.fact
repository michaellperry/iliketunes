namespace ILikeTunes;

fact Individual {
key:
    string anonymousId;

mutable:
	publish string name;

query:
    Tune* tunes {
        Like l: l.individual = this
        Tune t : t = l.tune
    }
}

fact Tune {
key:
    string name;

query:
    Individual* individuals {
        Like l : l.tune = this
        Individual i : i = l.individual
    }
}

fact Like {
key:
    Individual individual;
    Tune tune;
}
